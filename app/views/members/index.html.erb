<p id="notice"><%= notice %></p>

<h1><%= t('members.listting') %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('members.name') %></th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
        <td><%= member.address %></td>
        <td><%= link_to t('members.show'), member %></td>
        <td><%= link_to t('members.edit'), edit_member_path(member) %></td>
        <td><%= link_to t('members.destroy'), member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('members.newmember'), new_member_path %>
